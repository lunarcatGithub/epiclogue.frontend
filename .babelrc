{
	"presets": ["next/babel"],
	"plugins": [
		[
        "styled-components",
            {
                "ssr": true, 
                "displayName": true,
                "preprocess": false
            }
        ],
        [
            "module-resolver",
            {
                "root": ["./"],
                "alias": {
                    "@component": "./src/component",
                    "@utils": "./src/utils",
                    "@hooks": "./src/hooks",
                    "@store": "./src/store",
                    "@reducer": "./src/reducer",
                    "@language": "./src/language"
                }
            }
        ],
        [
            "inline-react-svg"
        ]
	]
}